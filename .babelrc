{
  presets: [
    [
      '@babel/preset-env',
      {
        modules: false
      }
    ],
    '@babel/preset-react'
  ],
  plugins: [
    'react-hot-loader/babel',
    ["@babel/plugin-proposal-class-properties", { "loose": true }],
    ["@babel/plugin-transform-regenerator"],
    ["@babel/plugin-transform-runtime"]
  ]
}
